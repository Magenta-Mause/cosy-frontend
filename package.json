{
  "name": "cosy-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsr generate && bun --hot src/index.ts",
    "tsr:gen": "tsr generate",
    "start": "NODE_ENV=production bun src/index.ts",
    "build": "tsr generate && biome lint && bun run build.ts",
    "build:opt": "tsr generate && biome lint && bun run build.ts --minify --splitting",
    "lint": "biome lint && tsc",
    "lint:fix": "biome lint --write",
    "lint:fix:unsafe": "biome check --write --unsafe",
    "gen:api": "curl http://localhost:8080/api/v3/api-docs > openapi-backend.json && bunx orval"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-form": "^0.1.8",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@reduxjs/toolkit": "^2.11.0",
    "@tanstack/react-form": "^1.27.0",
    "@tanstack/react-query": "^4.0.0",
    "@tanstack/react-router": "^1.139.7",
    "@tanstack/react-router-devtools": "^1.139.10",
    "@tanstack/router-cli": "^1.139.7",
    "axios": "^1.13.2",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "i18next": "^25.6.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.545.0",
    "next-themes": "^0.4.6",
    "react": "^19",
    "react-dom": "^19",
    "react-i18next": "^16.3.5",
    "react-redux": "^9.2.0",
    "shell-quote": "^1.8.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@tanstack/router-plugin": "^1.139.7",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/shell-quote": "^1.7.5",
    "orval": "^7.17.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0"
  }
}
